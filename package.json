    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "reactstrap": "^8.4.1",
    "sqlite": "^4.0.14",
    "sqlite3": "^4.2.0"
  },
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "set BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "mkdir build/src & robocopy electron/ build/electron /S & robocopy src/shared build/src/shared /S ",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^10.2.0",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.4"
  },
  "build": {
    "files": [
      "build/**/*",
      "db/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "pruefungsanerkennung",
      "owner": "mcieslak"
    }
  }
